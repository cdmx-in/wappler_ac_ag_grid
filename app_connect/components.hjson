              "help": "Timezone for Date Fields"
            },
            {
              "name": "fixedHeader",
              "attribute": "fixedHeader",
              "title": "Sticky Header",
              "type": "boolean",
              "display": "fieldset",
              "noChangeOnHide": true,
              "groupEnabler": true,
              "defaultValue": false,
              "show": [
                "fixedHeaderOffset",
                "topbarClass",
                "fixedTopOffset"
              ],
              "children": [
                {
                  "name": "fixedHeaderOffset",
                  "attribute": "fixedHeaderOffset",
                  "title": "Header Offset",
                  "type": "number",
                  "initDisplay": "none",
                  "defaultValue": 100,
                  "help": "Specifies offset from top of viewport area (Optional)"
                },
                {
                  "name": "topbarClass",
                  "attribute": "topbarClass",
                  "title": "Topbar Class",
                  "type": "text",
                  "initDisplay": "none",
                  "defaultValue": "topbar",
                  "help": "Specify class eg: topbar to measure the offset from (Optional)"
                },
                {
                  "name": "fixedTopOffset",
                  "attribute": "fixedTopOffset",
                  "title": "Topbar Offset",
                  "type": "number",
                  "initDisplay": "none",
                  "defaultValue": 80,
                  "help": "Specify topbar offset, Default: 80"
                }
              ]
            },
            {
              "name": "rowHeight",
              "attribute": "rowHeight",
              "title": "Row Height",
              "type": "number",
              "defaultValue": 25,
              "help": "Height of each row in pixels"
            },
            {
              "name": "headerHeight",
              "attribute": "headerHeight",
              "title": "Header Height",
              "type": "number",
              "defaultValue": 30,
              "help": "Height of the header row in pixels"
            },
            {
              "name": "suppressRowClickSelection",
              "attribute": "suppressRowClickSelection",
              "title": "Suppress Row Click Selection",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables row selection on row click"
            },
            {
              "name": "suppressMenuHide",
              "attribute": "suppressMenuHide",
              "title": "Suppress Menu Hide",
              "type": "boolean",
              "defaultValue": false,
              "help": "Prevents hiding the column menu"
            },
            {
              "name": "suppressMovableColumns",
              "attribute": "suppressMovableColumns",
              "title": "Suppress Movable Columns",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables moving columns"
            },
            {
              "name": "enableCellExpressions",
              "attribute": "enableCellExpressions",
              "title": "Enable Cell Expressions",
              "type": "boolean",
              "defaultValue": false,
              "help": "Enables expressions in cell values"
            },
            {
              "name": "animateRows",
              "attribute": "animateRows",
              "title": "Animate Rows",
              "type": "boolean",
              "defaultValue": false,
              "help": "Enables row animation on data changes"
            },
            {
              "name": "suppressAggFuncInHeader",
              "attribute": "suppressAggFuncInHeader",
              "title": "Suppress Aggregation Function in Header",
              "type": "boolean",
              "defaultValue": false,
              "help": "Hides the aggregation function in column headers"
            },
            {
              "name": "suppressAggAtRootLevel",
              "attribute": "suppressAggAtRootLevel",
              "title": "Suppress Aggregation at Root Level",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables aggregation at the root level"
            },
            {
              "name": "suppressClipboardPaste",
              "attribute": "suppressClipboardPaste",
              "title": "Suppress Clipboard Paste",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables pasting data from clipboard"
