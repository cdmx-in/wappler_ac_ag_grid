{
  "components": [
    {
      "type": "dmx-ag-grid",
      "selector": "dmx-ag-grid, [is=dmx-ag-grid]",
      "groupTitle": "Components",
      "groupIcon": "fa fa-lg fa-cube",
      "title": "AG Grid: @@id@@",
      "icon": "fa fa-lg fa-table",
      "state": "opened",
      "anyParent": true,
      "template": "<dmx-ag-grid id=\"@@id@@\"></dmx-ag-grid>",
      "baseName": "aggrid",
      "help": "AG Grid component for data display",
      "dataScheme": [
        {
          "name": "id",
          "title": "Id",
          "type": "number"
        },
        {
          "name": "data",
          "title": "Data",
          "type": "object"
        },
        {
          "name": "state",
          "title": "state",
          "type": "object",
          sub: [
              {name: "gridReady", type: "boolean"},
              {name: "firstDataRendered", type: "boolean"},
              {name: "rowDataUpdated", type: "boolean"}
          ]
        },
        {
          "name": "count",
          "title": "Count",
          "type": "number"
        },
        {
          "name": "fields",
          "title": "Fields",
          "type": "object"
        },
        {
          "name": "fileData",
          "title": "fileData",
          "type": "array"
        },
        {
          "name": "selectedRows",
          "title": "selectedRows",
          "type": "array"
        }
      ],
      "outputType": "object",
      "dataPick": true,
      "properties": [
        {
          "group": "AG Grid Properties",
          "variables": [
            {
              "name": "id",
              "attribute": "id",
              "title": "ID",
              "type": "text",
              "required": true,
              "help": "Unique ID"
            },
            {
              "name": "rowData",
              "attribute": "dmx-bind:data",
              "title": "Data Source",
              "type": "text",
              "dataBindings": true,
              "defaultValue": [],
              "help": "Enter Data Source"
            },
            { 
            "name": "gridNoLoad", 
            "hasAttribute": "noload", 
            "title": "No Auto Load", 
            "type": "boolean", 
            "defaultValue": "", 
            "initValue": true
            },
            {
              "name": "grid_theme",
              "attribute": "grid_theme",
              "title": "Theme",
              "type": "droplist",
              "defaultValue": 'ag-theme-alpine',
              "values": [
              {title: 'Alpine', value: 'ag-theme-alpine'},
              {title: 'Balham', value: 'ag-theme-balham'},
              {title: 'Material', value: 'ag-theme-material'},
              {title: 'Quartz', value: 'ag-theme-quartz'},
              {title: 'Custom', value: 'ag-theme-alpine ag-theme-custom'},
              {title: 'Base', value: 'ag-grid'}
              ],
              "help": "Select a theme for the grid"
            },
            {
              "name": "dark_mode",
              "attribute": "dmx-bind:dark_mode",
              "title": "Dark Mode",
              "type": "boolean",
              "defaultValue": false,
              "help": "Enables dark mode"
            },
            {
              "name": "locale_text",
              "attribute": "locale_text",
              "title": "Locale",
              "type": "droplist",
              "defaultValue": 'EN',
              "values": [
              {title: 'English', value: 'EN'},
              {title: 'Hebrew', value: 'HE'},
              {title: 'Russian', value: 'RU'},
              {title: 'Portuguese', value: 'PT'}
              ],
              "help": "Select locale for the Grid, Currently Supproted Hebrew (HE), Russian (RU) and English (EN) locale"
            }
          ]
        },
        {
          "group": "AG Grid Options",
          "variables": [
            {
              "name": "minWidth",
              "attribute": "min_width",
              "title": "Minimum Width",
              "type": "number",
              "defaultValue": 150,
              "help": "The minimum width of the column"
            },
            {
              "name": "resizable",
              "attribute": "dmx-bind:resizable",
              "title": "Resizable",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column can be resized"
            },
            {
              "name": "sortable",
              "attribute": "dmx-bind:sortable",
              "title": "Sortable",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column are sortable"
            },
            {
              "name": "ci_sort",
              "attribute": "dmx-bind:ci_sort",
              "title": "Case-Insensitive Sort",
              "type": "boolean",
              "defaultValue": false,
              "help": "Specifies if the columns are sorted in case-insensitive manner"
            },
            {
              "name": "filter",
              "attribute": "dmx-bind:filter",
              "title": "Filter",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column has a filter"
            },
            {
              "name": "floatingFilter",
              "attribute": "dmx-bind:floating_filter",
              "title": "Floating Filter",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column has a floating filter"
            },
            {
              "name": "enableRtl",
              "attribute": "dmx-bind:enable_rtl",
              "title": "Enable RTL",
              "type": "boolean",
              "defaultValue": false,
              "help": "Enabled Right to Left, used for Hebrew and Arabic"
            },
            {
              "name": "columnHoverHighlight",
              "attribute": "dmx-bind:column_hover_highlight",
              "title": "Column Hover Highlight",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies Column Hover Highlighting"
            },
            {
              "name": "suppressRowDeselection",
              "attribute": "dmx-bind:suppress_row_deselection",
              "title": "Supress Row Deselection",
              "type": "boolean",
              "defaultValue": false,
              "help": "Specifies if rows can be deselected"
            },
            {
              "name": "pagination",
              "attribute": "dmx-bind:pagination",
              "title": "Pagination",
              "type": "boolean",
              "defaultValue": true,
              "help": "Enables pagination"
            },
            {
              "name": "paginationAutoPageSize",
              "attribute": "pagination_auto_page_size",
              "title": "Auto Pagination",
              "type": "boolean",
              "defaultValue": false,
              "help": "Built-in pagination size as per page size."
            },
            {
              "name": "exportToCSV",
              "attribute": "dmx-bind:export_to_csv",
              "title": "CSV Export",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if Export to CSV to be enabled"
            },
            {
              "name": "exportToPDF",
              "attribute": "dmx-bind:export_to_pdf",
              "title": "PDF Export",
              "type": "boolean",
              "defaultValue": false,
              "help": "Specifies if Export to PDF to be enabled"
            },
            {
              "name": "exportExcludeHidden",
              "attribute": "dmx-bind:export_exclude_hidden_fields",
              "title": "Exclude Hidden Fields",
              "type": "boolean",
              "defaultValue": false,
              "help": "Specifies if Hidden fields are to be excluded in CSV export."
            },
            {
              "name": "exportExcludeFields",
              "attribute": "export_exclude_fields",
              "title": "Exclude Fields CSV",
              "type": "text",
              "help": "Specifies fields to be excluded in CSV export (comma-seperated)."
            },
            {
              "name": "paginationPageSizeSelector",
              "attribute": "dmx-bind:pagination_page_size_selector",
              "title": "Page Limit Selectors",
              "type": "text",
              "defaultValue": "[20,50,100]",
              "help": "Allowed selectors for for page size."
            },
            {
              "name": "paginationPageSize",
              "attribute": "pagination_page_size",
              "title": "Page Limit",
              "type": "number",
              "defaultValue": 20,
              "help": "Number of rows to show per page"
            },
            {
              "name": "rowSelection",
              "attribute": "row_selection",
              "title": "Row Selection",
              "type": "droplist",
              "defaultValue": 'multiple',
              "values": [
              {title: 'Single', value: 'single'},
              {title: 'Muliple', value: 'multiple'}
              ],
              "help": "Row Selection (single or multiple)"
            },
            {
              "name": "timezone",
              "attribute": "timezone",
              "title": "Timezone",
              "type": "droplist",
              "defaultValue": '',
              "values": [
              {title: 'Local', value: ''},
              {title: 'UTC - Coordinated Universal Time', value: 'UTC'},
              {title: 'GMT - Greenwich Mean Time', value: 'GMT'},
              {title: 'PST - Pacific Standard Time', value: 'PST'},
              {title: 'PDT - Pacific Daylight Time', value: 'PDT'},
              {title: 'EST - Eastern Standard Time', value: 'EST'},
              {title: 'EDT - Eastern Daylight Time', value: 'EDT'},
              {title: 'CST - Central Standard Time', value: 'CST'},
              {title: 'CDT - Central Daylight Time', value: 'CDT'},
              {title: 'MST - Mountain Standard Time', value: 'MST'},
              {title: 'MDT - Mountain Daylight Time', value: 'MDT'},
              {title: 'HST - Hawaii Standard Time', value: 'HST'},
              {title: 'AKST - Alaska Standard Time', value: 'AKST'},
              {title: 'AKDT - Alaska Daylight Time', value: 'AKDT'},
              {title: 'AEST - Australian Eastern Standard Time', value: 'AEST'},
              {title: 'AEDT - Australian Eastern Daylight Time', value: 'AEDT'},
              {title: 'ACST - Australian Central Standard Time', value: 'ACST'},
              {title: 'ACDT - Australian Central Daylight Time', value: 'ACDT'},
              {title: 'AWST - Australian Western Standard Time', value: 'AWST'},
              {title: 'BST - British Summer Time', value: 'BST'},
              {title: 'IST - Indian Standard Time', value: 'IST'},
              {title: 'JST - Japan Standard Time', value: 'JST'},
              {title: 'CET - Central European Time', value: 'CET'},
              {title: 'CEST - Central European Summer Time', value: 'CEST'},
              {title: 'EET - Eastern European Time', value: 'EET'},
              {title: 'EEST - Eastern European Summer Time', value: 'EEST'},
              {title: 'MSK - Moscow Standard Time', value: 'MSK'},
              {title: 'IST - Israel Standard Time', value: 'IST'},
              {title: 'NZST - New Zealand Standard Time', value: 'NZST'},
              {title: 'NZDT - New Zealand Daylight Time', value: 'NZDT'},
              {title: 'NST - Newfoundland Standard Time', value: 'NST'},
              {title: 'NDT - Newfoundland Daylight Time', value: 'NDT'},
              {title: 'AST - Atlantic Standard Time', value: 'AST'},
              {title: 'ADT - Atlantic Daylight Time', value: 'ADT'},
              {title: 'GMT+1 - Central European Standard Time', value: 'GMT+1'},
              {title: 'GMT+2 - Eastern European Standard Time', value: 'GMT+2'},
              {title: 'GMT+3 - Moscow Standard Time', value: 'GMT+3'},
              {title: 'GMT+4 - Gulf Standard Time', value: 'GMT+4'},
              {title: 'GMT+5 - Pakistan Standard Time', value: 'GMT+5'},
              {title: 'GMT+6 - Central Asia Standard Time', value: 'GMT+6'},
              {title: 'GMT+7 - Southeast Asia Standard Time', value: 'GMT+7'},
              {title: 'GMT+8 - China Standard Time', value: 'GMT+8'},
              {title: 'GMT+9 - Japan Standard Time', value: 'GMT+9'},
              {title: 'GMT+10 - Australian Eastern Standard Time', value: 'GMT+10'},
              {title: 'GMT+11 - Solomon Islands Time', value: 'GMT+11'},
              {title: 'GMT+12 - New Zealand Standard Time', value: 'GMT+12'}
              ],
              "help": "Timezone for Date Fields"
            },
            {
              "name": "dateFormat",
              "attribute": "date_format",
              "title": "Date Format",
              "type": "text",
              "defaultValue": "dd/MM/yyyy hh:mm A",
              "help": "Date Format"
            },
            {
              "name": "quickFilterField",
              "attribute": "quick_filter_field",
              "title": "Filter Field ID",
              "type": "text",
              "defaultValue": "search_field",
              "help": "Specifies the field ID of the search field when using grid quick filter"
            },
            {
              "name": "loadingOverlay",
              "attribute": "dmx-bind:loading_overlay",
              "title": "Enable Loading Overlay",
              "type": "boolean",
              "defaultValue": false,
              "help": "This enables loading overlay",
              "display": "fieldset",
              "show": [
                "loadingOverlayDuration"
              ],
              "children": [
                {
                  "name": "loadingOverlayDuration",
                  "attribute": "loading_overlay_duration",
                  "title": "Loading Overlay Duration",
                  "type": "number",
                  "defaultValue": 500,
                  "help": "Loading Overlay Duration in ms"
                }
              ]
            },
            {
              "name": "fixedHeader",
              "title": "Sticky Header and Horizontal Scrollbar",
              "attribute": "f_header",
              "type": "boolean",
              "display": "fieldset",
              "noChangeOnHide": true,
              "groupEnabler": true,
              "defaultValue": false,
              "show": [
                "enableFixedHeader",
                "fixedHeaderOffset",
                "topbarClass",
                "fixedTopOffset",
                "fixedHorizontalScroll",
                "fixedHorizontalScrollWidth"
              ],
              "children": [
                {
                  "name": "enableFixedHeader",
                  "attribute": "dmx-bind:fixed_header",
                  "title": "Fixed Header",
                  "type": "boolean",
                  "initDisplay": "none",
                  "defaultValue": false,
                  "help": "Enables Sticky header"
                },
                {
                  "name": "fixedHorizontalScroll",
                  "attribute": "dmx-bind:fixed_horizontal_scroll",
                  "title": "Fixed Horizontal Scroll",
                  "type": "boolean",
                  "initDisplay": "none",
                  "defaultValue": false,
                  "help": "Enables bottom fixed horizontal scroll"
                },
                {
                  "name": "fixedHeaderOffset",
                  "attribute": "fixed_header_offset",
                  "title": "Header Offset",
                  "type": "number",
                  "initDisplay": "none",
                  "defaultValue": 100,
                  "help": "Specifies offset from top of viewport area (Optional)"
                },
                {
                  "name": "topbarClass",
                  "attribute": "topbar_class",
                  "title": "Topbar Class",
                  "type": "text",
                  "initDisplay": "none",
                  "defaultValue": "topbar",
                  "help": "Specify class eg: topbar to measure the offset from (Optional)"
                },
                {
                  "name": "fixedTopOffset",
                  "attribute": "fixed_top_offset",
                  "title": "Topbar Offset",
                  "type": "number",
                  "initDisplay": "none",
                  "defaultValue": 80,
                  "help": "Specify topbar offset, Default: 80"
                }
              ]
            },
            {
              "name": "rowHeight",
              "attribute": "row_height",
              "title": "Row Height",
              "type": "number",
              "defaultValue": null,
              "help": "Height of each row in pixels"
            },
            {
              "name": "headerHeight",
              "attribute": "header_height",
              "title": "Header Height",
              "type": "number",
              "defaultValue": null,
              "help": "Height of the header row in pixels"
            },
            {
              "name": "wrapHeaderText",
              "attribute": "dmx-bind:wrap_header_text",
              "title": "Wrap Header Text",
              "type": "boolean",
              "defaultValue": true,
              "help": "This will cause long headers to wrap."
            },
            {
              "name": "autoHeaderHeight",
              "attribute": "dmx-bind:auto_header_height",
              "title": "Auto Header Height",
              "type": "boolean",
              "defaultValue": true,
              "help": "The header row height is automatically set based on the content of the header."
            },
            {
              "name": "wrapText",
              "attribute": "dmx-bind:wrap_text",
              "title": "Wrap Text",
              "type": "boolean",
              "defaultValue": false,
              "help": "This will cause long texts to wrap in the cells."
            },
            {
              "name": "vCenterCellData",
              "attribute": "dmx-bind:vert_center_cell_data",
              "title": "Center Align (V)",
              "type": "boolean",
              "defaultValue": false,
              "help": "Specifies if the cell data should be vertically centered."
            },
            {
              "name": "autoHeight",
              "attribute": "dmx-bind:auto_height",
              "title": "Auto Height",
              "type": "boolean",
              "defaultValue": false,
              "help": "The height is automatically set based on the content of the cells."
            },
            {
              "name": "suppressRowClickSelection",
              "attribute": "dmx-bind:suppress_row_click_selection",
              "title": "Suppress Row Click Selection",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables row selection on row click"
            },
            {
              "name": "suppressMenuHide",
              "attribute": "dmx-bind:suppress_menu_hide",
              "title": "Suppress Menu Hide",
              "type": "boolean",
              "defaultValue": false,
              "help": "Prevents hiding the column menu"
            },
            {
              "name": "suppressMovableColumns",
              "attribute": "dmx-bind:suppress_movable_columns",
              "title": "Suppress Movable Columns",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables moving columns"
            },
            {
              "name": "enableCellExpressions",
              "attribute": "dmx-bind:enable_cell_expressions",
              "title": "Enable Cell Expressions",
              "type": "boolean",
              "defaultValue": false,
              "help": "Enables expressions in cell values"
            },
            {
              "name": "animateRows",
              "attribute": "dmx-bind:animate_rows",
              "title": "Animate Rows",
              "type": "boolean",
              "defaultValue": false,
              "help": "Enables row animation on data changes"
            },
            {
              "name": "suppressAggFuncInHeader",
              "attribute": "dmx-bind:suppress_agg_func_in_header",
              "title": "Suppress Aggregation Function in Header",
              "type": "boolean",
              "defaultValue": false,
              "help": "Hides the aggregation function in column headers"
            },
            {
              "name": "suppressClipboardPaste",
              "attribute": "dmx-bind:suppress_clipboard_paste",
              "title": "Suppress Clipboard Paste",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables pasting data from clipboard"
            },
            {
              "name": "suppressScrollOnNewData",
              "attribute": "dmx-bind:suppress_scroll_on_new_data",
              "title": "Suppress Scroll on New Data",
              "type": "boolean",
              "defaultValue": false,
              "help": "Prevents scrolling to newly added data"
            },
            {
              "name": "suppressPropertyNamesCheck",
              "attribute": "dmx-bind:suppress_property_names_check",
              "title": "Suppress Property Names Check",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables checking for duplicate property names"
            },
            {
              "name": "hideIdField",
              "attribute": "dmx-bind:hide_id_field",
              "title": "Hide ID Field",
              "type": "boolean",
              "defaultValue": false,
              "help": "Hides the ID Field in Grid"
            },
            {
              "name": "numericColumnAlign",
              "attribute": "dmx-bind:numeric_column_align",
              "title": "Right Align Numeric Columns",
              "type": "boolean",
              "defaultValue": false,
              "help": "Align numeric columns to the right."
            },
            {
              "name": "rowClickEvent",
              "attribute": "dmx-bind:row_click_event",
              "title": "Row Click Events",
              "type": "boolean",
              "defaultValue": false,
              "help": "This enables row click events which can then be used in Dynamic Events => Grid Events => Row Clicked"
            },
            {
              "name": "rowDoubleClickEvent",
              "attribute": "dmx-bind:row_double_click_event",
              "title": "Row Double Click Events",
              "type": "boolean",
              "defaultValue": false,
              "help": "This enables row double click events which can then be used in Dynamic Events => Grid Events => Row Double Clicked"
            },
            {
              "name": "rowCheckboxEvent",
              "attribute": "dmx-bind:row_checkbox_event",
              "title": "Enable Row Selection",
              "type": "boolean",
              "defaultValue": false,
              "help": "This enables row selection which can then be used in Dynamic Events => Grid Events => Checkbox Checked || Checkbox Unchecked"
            },
            {
              "name": "rowStatusEvent",
              "attribute": "dmx-bind:row_status_event",
              "title": "Enable Row Status Toggle",
              "type": "boolean",
              "defaultValue": false,
              "help": "This enables row status events which can then be used in Dynamic Events => Grid Events => Checkbox Checked || Checkbox Unchecked"
            }
          ]
        },
        {
          "group": "📒 Data Type Overrides",
          "variables": [
            {
              "name": "ctypes",
              "title": "Configure Type Overrides",
              "attributeStartsWith": "dmx-bind",
              "attribute": "ctypes",
              "type": "boolean",
              "defaultValue": false,
              "icon": "fa fa-lg fa-database",
              "groupIcon": "fa fa-lg fa-exchange",
              "display": "fieldset",
              "show": [
                "typeOverrides"
              ],
              "noChangeOnHide": true,
              "groupEnabler": true,
              "help": "This allows you to override the auto-detected datatypes"
              "children": [
                {
                  "name": "typeOverrides",
                  "attribute": "dmx-bind:ctypes",
                  "title": "Type Overrides",
                  "type": "grid",
                  "jsonFormat": true,
                  "encloseBT": true,
                  "jsonBT": true,
                  "defaultValue": [],
                  "initDisplay": "none",
                  "columns": [
                    {
                      "field": "field",
                      "caption": "Field",
                      "size": "20%",
                      "editable": {
                        "type": "text"
                      }
                    },
                    {
                      "field": "type",
                      "caption": "Type",
                      "size": "35%",
                      "editable": {
                        "type": "list", 
                        "items": [
                          {id: 'number', text: 'number'},
                          {id: 'text', text: 'text'},
                          {id: 'date', text: 'date'}
                        ]
                      }
                    },
                  ],
                  "newRecord": {
                    "name": "",
                    "field": "",
                    "type": ""
                  }
                }
              ]
            }
          ]
        },
        {
          "group": "📒 Style Formatting",
          "variables": [
            {
              "name": "cstyles",
              "title": "Configure Colors and Fonts",
              "attributeStartsWith": "dmx-bind",
              "attribute": "cstyles",
              "type": "boolean",
              "defaultValue": false,
              "display": "fieldset",
              "show": [
                "listColors"
              ],
              "noChangeOnHide": true,
              "groupEnabler": true,
              "help": "Add custom colors to column data based on condition, ex: field: status, condition: status==false, Color: red"
              "children": [
                {
                  "name": "listColors",
                  "attribute": "dmx-bind:cstyles",
                  "title": "Text and Cell Colors",
                  "type": "grid",
                  "dataBindings": true,
                  "jsonFormat": true,
                  "encloseBT": true,
                  "jsonBT": true,
                  "initDisplay": "none",
                  "columns": [
                    {
                      "field": "field",
                      "caption": "Field",
                      "editable": {
                        "type": "text"
                      }
                    },
                    {
                      "field": "condition",
                      "caption": "Condition",
                      "editable": {
                        "type": "text"
                      }
                    },
                    {
                      "field": "customColor",
                      "caption": "Color",
                      "editable": {
                        "type": "text"
                      },
                      help: "Example: '#FF0000' or 'red'"
                    },
                    {
                      "field": "area",
                      "caption": "Area",
                      "editable": {
                        "type": "list", 
                        "items": [
                          {id: 'text', text: 'text'},
                          {id: 'cell', text: 'cell'}
                        ]
                      }
                    },
                    {
                      "field": "font",
                      "caption": "Font",
                      "editable": {
                        "type": "list", 
                        "items": [
                          {id: 'normal', text: 'normal'},
                          {id: 'italic', text: 'italic'},
                          {id: 'bold', text: 'bold'}
                        ]
                      }
                    }
                  ],
                  "newRecord": {
                    "field": "",
                    "condition": "",
                    "customColor": "",
                    "area": "text",
                    "font": "normal"
                  }
                }
              ]
            },
            {
              "name": "rstyles",
              "title": "Configure Row Colors",
              "attributeStartsWith": "dmx-bind",
              "attribute": "rstyles",
              "type": "boolean",
              "defaultValue": false,
              "display": "fieldset",
              "show": [
                "listRowColors"
              ],
              "noChangeOnHide": true,
              "groupEnabler": true,
              "help": "Add custom Colors function that will return true or false. Add the fucntion name in the Function column."
              "children": [
                {
                  "name": "listRowColors",
                  "attribute": "dmx-bind:rstyles",
                  "title": "Row Colors",
                  "type": "grid",
                  "key": "field",
                  "dataBindings": true,
