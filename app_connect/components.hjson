              {title: 'GMT+9 - Japan Standard Time', value: 'GMT+9'},
              {title: 'GMT+10 - Australian Eastern Standard Time', value: 'GMT+10'},
              {title: 'GMT+11 - Solomon Islands Time', value: 'GMT+11'},
              {title: 'GMT+12 - New Zealand Standard Time', value: 'GMT+12'}
