          ]
        },
        {
          "group": "📒 Style Formatting",
          "variables": [
            {
              "name": "cstyles",
              "attributeStartsWith": "dmx-bind",
              "attribute": "cstyles",
              "title": "Configure Colors",
              "type": "boolean",
              "display": "fieldset",
              "show": [
                "listColors"
              ],
              "noChangeOnHide": true,
              "groupEnabler": true,
              "help": "Add custom colors to column data based on condition, ex: field: status, condition: status==false, Color: red"
              "children": [
                {
                  "name": "help",
                  "type": "static",
                  "help": "Add custom colors for the fields"
                },
                {
                  "name": "listColors",
                  "attribute": "dmx-bind:cstyles",
                  "title": "Colors",
                  "type": "grid",
                  "dataBindings": true,
                  "key": "field",
                  "jsonFormat": true,
                  "encloseBT": true,
                  "jsonBT": true,
                  "initDisplay": "none",
                  "columns": [
                    {
                      "field": "field",
                      "caption": "Field",
                      "size": "20%",
                      "editable": {
                        "type": "text"
                      }
                    },
                    {
                      "field": "condition",
                      "caption": "Condition",
                      "size": "35%",
                      "editable": {
                        "type": "text"
                      }
                    },
                    {
                      field: "customColor",
                      caption: "Color",
                      size: "20%",
                      editable: {
                        type: "text"
                      },
                      help: "Example: '#FF0000' or 'red'"
                    }
                  ],
                  "newRecord": {
                    "name": "",
                    "value": "",
                    "field": "",
                    "condition": "",
                    "customColor": ""
                  }
                }
              ]
            }
          ]
        },
        {
