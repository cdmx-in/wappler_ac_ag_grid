{
  "components": [
    {
      "type": "dmx-ag-grid",
      "selector": "dmx-ag-grid, [is=dmx-ag-grid]",
      "groupTitle": "Components",
      "groupIcon": "fa fa-lg fa-cube",
      "title": "AG Grid: @@id@@",
      "icon": "fa fa-lg fa-table",
      "state": "opened",
      "anyParent": true,
      "template": "<dmx-ag-grid id=\"@@id@@\"></dmx-ag-grid>",
      "baseName": "aggrid",
      "help": "AG Grid component for data display",
      "dataScheme": [
        {
          "name": "id",
          "title": "Id",
          "type": "number"
        },
        {
          "name": "data",
          "title": "Data",
          "type": "object"
        },
        {
          "name": "count",
          "title": "Count",
          "type": "number"
        },
        {
          "name": "fields",
          "title": "Fields",
          "type": "object"
        }
      ],
      "outputType": "object",
      "dataPick": true,
      "properties": [
        {
          "group": "AG Grid Properties",
          "variables": [
            {
              "name": "id",
              "attribute": "id",
              "title": "ID",
              "type": "text",
              "required": true,
              "help": "Unique ID"
            },
            {
              "name": "rowData",
              "attribute": "dmx-bind:data",
              "title": "Data Source",
              "type": "text",
              "dataBindings": true,
              "defaultValue": [],
              "required": true,
              "help": "Enter Data Source"
            },
            { 
            "name": "gridNoLoad", 
            "hasAttribute": "noload", 
            "title": "No Auto Load", 
            "type": "boolean", 
            "defaultValue": "", 
            "initValue": true
            },
            {
              "name": "grid_theme",
              "attribute": "grid_theme",
              "title": "Theme",
              "type": "droplist",
              "defaultValue": 'ag-theme-alpine',
              "values": [
              {title: 'Alpine', value: 'ag-theme-alpine'},
              {title: 'Balham', value: 'ag-theme-balham'},
              {title: 'Material', value: 'ag-theme-material'},
              {title: 'Base', value: 'ag-grid'}
              ],
              "help": "Select a theme for the grid"
            },
            {
              "name": "locale_text",
              "attribute": "locale_text",
              "title": "Locale",
              "type": "droplist",
              "defaultValue": 'EN',
              "values": [
              {title: 'English', value: 'EN'},
              {title: 'Hebrew', value: 'HE'}
              ],
              "help": "Select locale for the Grid, Currently Supproted Hebrew (HE) and English (EN) locale"
            }
          ]
        },
        {
          "group": "AG Grid Options",
          "variables": [
            {
              "name": "minWidth",
              "attribute": "min_width",
              "title": "Minimum Width",
              "type": "number",
              "defaultValue": 150,
              "help": "The minimum width of the column"
            },
            {
              "name": "resizable",
              "attribute": "dmx-bind:resizable",
              "title": "Resizable",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column can be resized"
            },
            {
              "name": "sortable",
              "attribute": "dmx-bind:sortable",
              "title": "Sortable",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column are sortable"
            },
            {
              "name": "filter",
              "attribute": "dmx-bind:filter",
              "title": "Filter",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column has a filter"
            },
            {
              "name": "floatingFilter",
              "attribute": "dmx-bind:floating_filter",
              "title": "Floating Filter",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column has a floating filter"
            },
            {
              "name": "exportToCSV",
              "attribute": "dmx-bind:export_to_csv",
              "title": "CSV Export",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if Export to CSV to be enabled"
            },
            {
              "name": "enableRtl",
              "attribute": "dmx-bind:enable_rtl",
              "title": "Enable RTL",
              "type": "boolean",
              "defaultValue": false,
              "help": "Enabled Right to Left, used for Hebrew and Arabic"
            },
            {
              "name": "columnHoverHighlight",
              "attribute": "dmx-bind:column_hover_highlight",
              "title": "Column Hover Highlight",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies Column Hover Highlighting"
            },
            {
              "name": "suppressRowDeselection",
              "attribute": "dmx-bind:suppress_row_deselection",
              "title": "Supress Row Deselection",
              "type": "boolean",
              "defaultValue": false,
              "help": "Specifies if rows can be deselected"
            },
            {
              "name": "pagination",
              "attribute": "dmx-bind:pagination",
              "title": "Pagination",
              "type": "boolean",
              "defaultValue": true,
              "help": "Enables pagination"
            },
            {
              "name": "paginationPageSize",
              "attribute": "pagination_page_size",
              "title": "Pagination Page Size",
              "type": "number",
              "defaultValue": 20,
              "help": "Number of rows to show per page"
            },
            {
              "name": "rowSelection",
              "attribute": "row_selection",
              "title": "Row Selection",
              "type": "droplist",
              "defaultValue": 'multiple',
              "values": [
              {title: 'Single', value: 'single'},
              {title: 'Muliple', value: 'multiple'}
              ],
              "help": "Row Selection (single or multiple)"
            },
            {
              "name": "timezone",
              "attribute": "timezone",
              "title": "Timezone",
              "type": "droplist",
              "defaultValue": '',
              "values": [
              {title: 'Local', value: ''},
              {title: 'UTC - Coordinated Universal Time', value: 'UTC'},
              {title: 'GMT - Greenwich Mean Time', value: 'GMT'},
              {title: 'PST - Pacific Standard Time', value: 'PST'},
              {title: 'PDT - Pacific Daylight Time', value: 'PDT'},
              {title: 'EST - Eastern Standard Time', value: 'EST'},
              {title: 'EDT - Eastern Daylight Time', value: 'EDT'},
              {title: 'CST - Central Standard Time', value: 'CST'},
              {title: 'CDT - Central Daylight Time', value: 'CDT'},
              {title: 'MST - Mountain Standard Time', value: 'MST'},
              {title: 'MDT - Mountain Daylight Time', value: 'MDT'},
              {title: 'HST - Hawaii Standard Time', value: 'HST'},
              {title: 'AKST - Alaska Standard Time', value: 'AKST'},
              {title: 'AKDT - Alaska Daylight Time', value: 'AKDT'},
              {title: 'AEST - Australian Eastern Standard Time', value: 'AEST'},
              {title: 'AEDT - Australian Eastern Daylight Time', value: 'AEDT'},
              {title: 'ACST - Australian Central Standard Time', value: 'ACST'},
              {title: 'ACDT - Australian Central Daylight Time', value: 'ACDT'},
              {title: 'AWST - Australian Western Standard Time', value: 'AWST'},
              {title: 'BST - British Summer Time', value: 'BST'},
              {title: 'IST - Indian Standard Time', value: 'IST'},
              {title: 'JST - Japan Standard Time', value: 'JST'},
              {title: 'CET - Central European Time', value: 'CET'},
              {title: 'CEST - Central European Summer Time', value: 'CEST'},
              {title: 'EET - Eastern European Time', value: 'EET'},
              {title: 'EEST - Eastern European Summer Time', value: 'EEST'},
              {title: 'MSK - Moscow Standard Time', value: 'MSK'},
              {title: 'IST - Israel Standard Time', value: 'IST'},
              {title: 'NZST - New Zealand Standard Time', value: 'NZST'},
              {title: 'NZDT - New Zealand Daylight Time', value: 'NZDT'},
              {title: 'NST - Newfoundland Standard Time', value: 'NST'},
              {title: 'NDT - Newfoundland Daylight Time', value: 'NDT'},
              {title: 'AST - Atlantic Standard Time', value: 'AST'},
              {title: 'ADT - Atlantic Daylight Time', value: 'ADT'},
              {title: 'GMT+1 - Central European Standard Time', value: 'GMT+1'},
              {title: 'GMT+2 - Eastern European Standard Time', value: 'GMT+2'},
              {title: 'GMT+3 - Moscow Standard Time', value: 'GMT+3'},
              {title: 'GMT+4 - Gulf Standard Time', value: 'GMT+4'},
              {title: 'GMT+5 - Pakistan Standard Time', value: 'GMT+5'},
              {title: 'GMT+6 - Central Asia Standard Time', value: 'GMT+6'},
              {title: 'GMT+7 - Southeast Asia Standard Time', value: 'GMT+7'},
              {title: 'GMT+8 - China Standard Time', value: 'GMT+8'},
              {title: 'GMT+9 - Japan Standard Time', value: 'GMT+9'},
              {title: 'GMT+10 - Australian Eastern Standard Time', value: 'GMT+10'},
              {title: 'GMT+11 - Solomon Islands Time', value: 'GMT+11'},
              {title: 'GMT+12 - New Zealand Standard Time', value: 'GMT+12'}
