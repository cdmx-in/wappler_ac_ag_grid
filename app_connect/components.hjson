              "help": "Timezone for Date Fields"
            },
            {
              "name": "dateFormat",
              "attribute": "date_format",
              "title": "Date Format",
              "type": "text",
              "defaultValue": "dd/MM/yyyy hh:mm A",
              "help": "Date Format"
            },
            {
              "name": "fixedHeader",
              "title": "Sticky Header and Horizontal Scrollbar",
              "type": "boolean",
              "display": "fieldset",
              "noChangeOnHide": true,
              "groupEnabler": true,
              "defaultValue": false,
              "show": [
                "enableFixedHeader",
                "fixedHeaderOffset",
                "topbarClass",
                "fixedTopOffset",
                "fixedHorizonalScroll",
                "fixedHorizonalScrollWidth"
              ],
              "children": [
                {
                  "name": "enableFixedHeader",
                  "attribute": "dmx-bind:fixed_header",
                  "title": "Fixed Header",
                  "type": "boolean",
                  "initDisplay": "none",
                  "defaultValue": false,
                  "help": "Enables Sticky header"
                },
                {
                  "name": "fixedHeaderOffset",
                  "attribute": "fixed_header_offset",
                  "title": "Header Offset",
                  "type": "number",
                  "initDisplay": "none",
                  "defaultValue": 100,
                  "help": "Specifies offset from top of viewport area (Optional)"
                },
                {
                  "name": "topbarClass",
                  "attribute": "topbar_class",
                  "title": "Topbar Class",
                  "type": "text",
                  "initDisplay": "none",
                  "defaultValue": "topbar",
                  "help": "Specify class eg: topbar to measure the offset from (Optional)"
                },
                {
                  "name": "fixedTopOffset",
                  "attribute": "fixed_top_offset",
                  "title": "Topbar Offset",
                  "type": "number",
                  "initDisplay": "none",
                  "defaultValue": 80,
                  "help": "Specify topbar offset, Default: 80"
                },
                {
                  "name": "fixedHorizontalScroll",
                  "attribute": "dmx-bind:fixed_horizontal_scroll",
                  "title": "Fixed Horizontal Scroll",
                  "type": "boolean",
                  "initDisplay": "none",
                  "defaultValue": false,
                  "help": "Enables bottom fixed horizontal scroll"
                },
                {
                  "name": "fixedHorizontalScrollWidth",
                  "attribute": "dmx-bind:fixed_horizontal_scroll_width",
                  "title": "Fixed Horizontal Scroll Width",
                  "type": "number",
                  "initDisplay": "none",
                  "defaultValue": 80,
                  "help": "Fixed horizontal scroll width percentage (Default: 80%)"
                }
              ]
            },
            {
              "name": "rowHeight",
              "attribute": "row_height",
              "title": "Row Height",
              "type": "number",
              "defaultValue": 25,
              "help": "Height of each row in pixels"
            },
            {
              "name": "headerHeight",
              "attribute": "header_height",
              "title": "Header Height",
              "type": "number",
              "defaultValue": 30,
              "help": "Height of the header row in pixels"
            },
            {
              "name": "suppressRowClickSelection",
              "attribute": "dmx-bind:suppress_row_click_selection",
              "title": "Suppress Row Click Selection",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables row selection on row click"
            },
            {
              "name": "suppressMenuHide",
              "attribute": "dmx-bind:suppress_menu_hide",
              "title": "Suppress Menu Hide",
              "type": "boolean",
              "defaultValue": false,
              "help": "Prevents hiding the column menu"
            },
            {
              "name": "suppressMovableColumns",
              "attribute": "dmx-bind:suppress_movable_columns",
              "title": "Suppress Movable Columns",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables moving columns"
            },
            {
              "name": "enableCellExpressions",
              "attribute": "dmx-bind:enable_cell_expressions",
              "title": "Enable Cell Expressions",
              "type": "boolean",
              "defaultValue": false,
              "help": "Enables expressions in cell values"
            },
            {
              "name": "animateRows",
              "attribute": "dmx-bind:animate_rows",
              "title": "Animate Rows",
              "type": "boolean",
              "defaultValue": false,
              "help": "Enables row animation on data changes"
            },
            {
              "name": "suppressAggFuncInHeader",
              "attribute": "dmx-bind:suppress_agg_func_in_header",
              "title": "Suppress Aggregation Function in Header",
              "type": "boolean",
              "defaultValue": false,
              "help": "Hides the aggregation function in column headers"
            },
            {
              "name": "suppressAggAtRootLevel",
              "attribute": "dmx-bind:suppress_agg_at_root_level",
              "title": "Suppress Aggregation at Root Level",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables aggregation at the root level"
            },
            {
              "name": "suppressClipboardPaste",
              "attribute": "dmx-bind:suppress_clipboard_paste",
              "title": "Suppress Clipboard Paste",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables pasting data from clipboard"
            },
            {
              "name": "suppressScrollOnNewData",
              "attribute": "dmx-bind:suppress_scroll_on_new_data",
              "title": "Suppress Scroll on New Data",
              "type": "boolean",
              "defaultValue": false,
              "help": "Prevents scrolling to newly added data"
            },
            {
              "name": "suppressPropertyNamesCheck",
              "attribute": "dmx-bind:suppress_property_names_check",
              "title": "Suppress Property Names Check",
              "type": "boolean",
              "defaultValue": false,
              "help": "Disables checking for duplicate property names"
            },
            {
              "name": "hideIdField",
              "attribute": "dmx-bind:hide_id_field",
              "title": "Hide ID Field",
              "type": "boolean",
              "defaultValue": false,
              "help": "Hides the ID Field in Grid"
            },
            {
              "name": "rowClickEvent",
              "attribute": "dmx-bind:row_click_event",
              "title": "Row Click Events",
              "type": "boolean",
              "defaultValue": false,
              "help": "This enabled row click events which can then be used in Dynamic events => Grid Events => Row Clicked"
            },
            {
              "name": "rowCheckboxEvent",
              "attribute": "dmx-bind:row_checkbox_event",
              "title": "Enable Row Selection",
              "type": "boolean",
              "defaultValue": false,
              "help": "This enables row selection which can then be used in Dynamic events => Grid Events => Checkbox Checked || Checkbox Unchecked"
            },
            {
              "name": "rowStatusEvent",
              "attribute": "dmx-bind:row_status_event",
              "title": "Enable Row Status Toggle",
              "type": "boolean",
              "defaultValue": false,
              "help": "This enabled row status events which can then be used in Dynamic events => Grid Events => Checkbox Checked || Checkbox Unchecked"
            }
          ]
        },
        {
          "group": "ðŸ“’ Data Type Overrides",
          "variables": [
            {
              "name": "ctypes",
              "title": "Configure Type Overrides",
              "attributeStartsWith": "dmx-bind",
              "attribute": "ctypes",
              "type": "boolean",
              "defaultValue": false,
              "icon": "fa fa-lg fa-database",
              "groupIcon": "fa fa-lg fa-exchange",
              "display": "fieldset",
              "show": [
                "typeOverrides"
              ],
              "noChangeOnHide": true,
              "groupEnabler": true,
              "help": "This allows you to override the auto-detected datatypes"
              "children": [
                {
                  "name": "typeOverrides",
                  "attribute": "dmx-bind:ctypes",
                  "title": "Type Overrides",
                  "type": "grid",
                  "jsonFormat": true,
                  "encloseBT": true,
                  "jsonBT": true,
                  "defaultValue": [],
                  "initDisplay": "none",
                  "columns": [
                    {
                      "field": "field",
                      "caption": "Field",
                      "size": "20%",
                      "editable": {
                        "type": "text"
                      }
                    },
                    {
                      "field": "type",
                      "caption": "Type",
                      "size": "35%",
                      "editable": {
                        "type": "list", 
                        "items": [
                          {id: 'number', text: 'number'},
                          {id: 'text', text: 'text'},
                          {id: 'date', text: 'date'}
                        ]
                      }
                    },
                  ],
                  "newRecord": {
                    "name": "",
                    "field": "",
                    "type": ""
                  }
                }
              ]
            }
          ]
        },
        {
          "group": "ðŸ“’ Style Formatting",
