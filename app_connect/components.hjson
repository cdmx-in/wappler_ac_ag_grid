      "groupTitle": "Components",
      "groupIcon": "fa fa-lg fa-cube",
      "title": "AG Grid: @@id@@",
      "icon": "fa fa-lg fa-table",
      "state": "opened",
      "anyParent": true,
      "template": "<dmx-ag-grid id=\"@@id@@-grid\">\n<div id=\"@@id@@\" class=\"col ag-theme-alpine\">\n</div>\n</dmx-ag-grid>",
      "baseName": "aggrid",
      "help": "AG Grid component for data display",
      "dataScheme": [
        {
          "name": "data",
          "title": "Data",
          "type": "object"
        }
      ],
      "outputType": "object",
      "dataPick": true,
      "outputType": "object",
      "dataScheme": [
        {
          "name": "data",
          "title": "Data",
          "type": "object"
        }
      ],
      "properties": [
        {
          "group": "AG Grid Properties",
          "variables": [
            {
              "name": "id",
              "attribute": "id",
              "title": "ID",
              "type": "text",
              "required": true,
              "help": "Unique ID"
            },
            {
              "name": "rowData",
              "attribute": "dmx-bind:data",
              "title": "Data Source",
              "type": "text",
              "dataBindings": true,
              "defaultValue": [],
              "required": true,
              "help": "Enter Data Source"
            }
          ]
        },
        {
          "group": "AG Grid Options",
          "variables": [
            {
              "name": "defaultColDef",
              "attribute": "defaultColDef",
              "title": "Default Column Definition",
              "type": "object",
              "defaultValue": {},
              "help": "Default properties for all columns"
            },
            {
              "name": "columnTypes",
              "attribute": "columnTypes",
              "title": "Column Types",
              "type": "object",
              "defaultValue": {},
              "help": "Map of custom column types"
            },
            {
              "name": "rowSelection",
              "attribute": "rowSelection",
              "title": "Row Selection",
              "type": "string",
              "defaultValue": "single",
              "options": [
                "single",
                "multiple",
                "none"
              ],
              "help": "Defines the row selection mode"
            },
            {
              "name": "minWidth",
              "attribute": "minWidth",
              "title": "Minimum Width",
              "type": "number",
              "defaultValue": 150,
              "help": "The minimum width of the column"
            },
            {
              "name": "resizable",
              "attribute": "resizable",
              "title": "Resizable",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column can be resized"
            },
            {
              "name": "sortable",
              "attribute": "sortable",
              "title": "Sortable",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column are sortable"
            },
            {
              "name": "filter",
              "attribute": "filter",
              "title": "Filter",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column has a filter"
            },
            {
              "name": "floatingFilter",
              "attribute": "floatingFilter",
              "title": "Floating Filter",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if the column has a floating filter"
            },
            {
              "name": "exportToCSV",
              "attribute": "exportToCSV",
              "title": "CSV Export",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if Export to CSV to be enabled"
            },
            {
              "name": "columnHoverHighlight",
              "attribute": "columnHoverHighlight",
              "title": "Column Hover Highlight",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies Column Hover Highlighting"
            },
            {
              "name": "suppressRowDeselection",
              "attribute": "suppressRowDeselection",
              "title": "Supress Row Deselection",
              "type": "boolean",
              "defaultValue": true,
              "help": "Specifies if rows can be deselected"
            },
            {
              "name": "pagination",
