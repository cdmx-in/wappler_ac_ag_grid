                  "initDisplay": "none",
                  "columns": [
                    {
                      "field": "field",
                      "caption": "Field",
                      "size": "20%",
                      "editable": {
                        "type": "text"
                      }
                    },
                    {
                      "field": "condition",
                      "caption": "Condition",
                      "size": "35%",
                      "editable": {
                        "type": "text"
                      }
                    },
                    {
                      "field": "customColor",
                      "caption": "Color",
                      "size": "20%",
                      "editable": {
                        "type": "text"
                      },
                      help: "Example: '#FF0000' or 'red'"
                    },
                    {
                      "field": "area",
                      "caption": "Area",
                      "size": "20%",
                      "editable": {
                        "type": "list", 
                        "items": [
                          {id: 'text', text: 'text'},
                          {id: 'cell', text: 'cell'}
                        ]
                      }
                    },
                    {
                      "field": "font",
                      "caption": "Font",
                      "size": "20%",
                      "editable": {
                        "type": "list", 
                        "items": [
                          {id: 'normal', text: 'normal'},
                          {id: 'italic', text: 'italic'},
                          {id: 'bold', text: 'bold'}
                        ]
                      }
                    }
                  ],
                  "newRecord": {
                    "field": "",
                    "condition": "",
                    "customColor": "",
                    "area": "text",
                    "font": "normal"
                  }
                }
              ]
            }
          ]
        },
